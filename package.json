{
  "name": "react-server-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "db:generate": "npx dotenv -e .env.local -- prisma generate",
    "db:studio": "npx dotenv -e .env.local -- prisma studio",
    "db:genpush": "yarn db:generate && yarn db:push",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
    "db:migrate": "npx dotenv -e .env.local -- prisma migrate dev"
  },
